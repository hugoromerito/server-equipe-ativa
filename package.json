{
  "name": "server-equipe-ativa",
  "version": "1.0.0",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "node --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
    "start": "node --env-file .env --experimental-strip-types --no-warnings src/server.ts",
    "db:seed": "node --env-file .env --experimental-strip-types --no-warnings src/db/seed.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "fastify",
    "postgresql",
    "drizzle-orm",
    "rest-api",
    "nodejs",
    "jwt",
    "aws-s3",
    "openai",
    "team-management",
    "organization-management"
  ],
  "author": "Hugo Queiroz",
  "license": "PROPRIETARY",
  "description": "API REST para gerenciamento de equipes ativas, organizações e demandas com TypeScript, Fastify e PostgreSQL",
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/node": "^24.0.12",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "testcontainers": "^11.5.1",
    "typescript": "^5.8.3",
    "ultracite": "5.0.32",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@casl/ability": "^6.7.3",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ai": "^4.3.19",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-orm": "^0.44.2",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.3",
    "postgres": "^3.4.7",
    "validation-br": "^1.5.2",
    "zod": "^3.25.76"
  }
}
